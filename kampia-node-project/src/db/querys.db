
SELECT t1.name, t2.number_plate, t2.type_vehicle
FROM users t1
RIGHT JOIN vehicles t2
ON t1.id = t2.owner;


SELECT  users.user, users.name, vehicles.type_vehicle, vehicles.number_plate, vehicles.device
FROM users
JOIN vehicles
ON users.user = vehicles.owner


